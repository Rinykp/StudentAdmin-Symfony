{% extends 'dashboard/index.html.twig' %}

{% block title %}Subject{% endblock %}

{% block content %}

<div class="col-sm-9">    <h1>Subject</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ subject.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ subject.name }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-success" href="{{ path('app_subject_index') }}">back to list</a>

    <a class="btn btn-warning" href="{{ path('app_subject_edit', {'id': subject.id}) }}">edit</a>

    {{ include('subject/_delete_form.html.twig') }}
</div>
{% endblock %}
